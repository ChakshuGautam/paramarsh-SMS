{
    "$schema": "https://turbo.build/schema.json",
    "tasks": {
        "build": {
            "dependsOn": [
                "^build"
            ],
            "outputs": [
                "dist/**",
                ".next/**"
            ]
        },
        "dev": {
            "cache": false,
            "persistent": true
        },
        "lint": {
            "outputs": []
        },
        "typecheck": {
            "outputs": []
        },
        "codegen": {
            "outputs": [
                "packages/openapi-types/**"
            ]
        }
    }
}