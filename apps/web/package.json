{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:resources": "jest test/resources/resource-validation.test.tsx",
    "test:validate": "jest test/resources/resource-validation.test.tsx --verbose",
    "test:enhanced": "jest test/resources/students/*-enhanced.test.tsx",
    "test:workflow": "jest test/resources/students/workflow-integration.test.tsx",
    "test:students": "jest test/resources/students/",
    "test:accessibility": "jest --testNamePattern='accessibility|Accessibility'",
    "test:performance": "jest --testNamePattern='performance|Performance'",
    "test:business-logic": "jest --testNamePattern='business.logic|Business.Logic'",
    "test:date-safety": "jest --testNamePattern='date|Date'",
    "test:multi-tenancy": "jest --testNamePattern='multi.tenancy|Multi.tenancy|tenant'",
    "e2e": "playwright test",
    "e2e:ui": "playwright test --ui",
    "e2e:headed": "playwright test --headed",
    "e2e:debug": "playwright test --debug",
    "e2e:students": "playwright test test/e2e/students/",
    "e2e:report": "playwright show-report",
    "cypress": "cypress open",
    "cypress:run": "cypress run",
    "cypress:headed": "cypress run --headed",
    "cypress:ci": "cypress run --record false",
    "cypress:snapshot": "cypress run --env visualRegressionType=base",
    "cypress:snapshot:update": "cypress run --env visualRegressionType=base --env updateSnapshots=true",
    "test:all": "npm run test && npm run cypress:run",
    "test:ci": "npm run test:coverage && npm run cypress:ci"
  },
