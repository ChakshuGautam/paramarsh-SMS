# Default environment variables for Paramarsh SMS API

# Server Configuration
PORT=3005

# Database Configuration
DATABASE_URL="postgresql://paramarsh:paramarsh123@localhost:5432/paramarsh_sms?schema=public"

# Multi-tenancy defaults
DEFAULT_BRANCH_ID=dps-main
DEFAULT_TENANT_ID=tenant1

# API Configuration
API_PREFIX=/api/v1

# Clerk Authentication (optional)
CLERK_PUBLISHABLE_KEY=
CLERK_SECRET_KEY=

# Authentication bypass for development (set to true to disable auth)
BYPASS_AUTH=false

# Node Environment
NODE_ENV=development